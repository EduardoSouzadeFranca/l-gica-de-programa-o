programa {
  
  funcao vazio contagem(inteiro a,inteiro b){
     
    se(b > a){
      retorne
    }
    escreva(b," ")
    contagem(a, b + 1)
  
  }
  
  funcao inicio() {
    
    inteiro n
     
    escreva("Informe o valor de N: ")
    leia(n)
    escreva("Números de 1 até ",n,": ")
    contagem(n, 1)
  
  }
}

  
      
   

 

