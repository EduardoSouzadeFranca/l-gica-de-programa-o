programa {
  funcao inicio() {
    
    inteiro valor
    escreva("valor inicial: ")
    leia(valor)
    
    inteiro valorfinal
    escreva("valor final: ")
    leia(valorfinal)
    
    para (inteiro contador = valor; contador <= valorfinal; contador++){
        escreva(contador,' ')
    }
  
    
  }
}
