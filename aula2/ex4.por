programa {
  funcao inicio() {
    inteiro angulo1 = 60
    inteiro angulo2 = 80 
    
    escreva ("Digite o primeiro ângulo: ", angulo1,"\n")
    escreva ("Digite o segundo ângulo: ", angulo2, "\n")

    inteiro angulo3 = 180 - (angulo1 + angulo2)

    escreva ("O terceiro ângulo do triângulo é: ", angulo3)
  }
}
